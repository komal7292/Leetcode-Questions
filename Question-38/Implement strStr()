//  Question-----

Input: haystack = "hello", needle = "ll"
Output: 2

// Answer------
var strStr = function(haystack, needle) {
   if(needle == null || haystack == null){
            return -1;
        }else if(needle === ""){
            return 0;
        }else if(haystack === ""){
            return -1;
        }else{
            return haystack.indexOf(needle);
        }
}
